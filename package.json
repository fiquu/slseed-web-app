  "scripts": {
    "semantic:build:css": "gulp build-css --gulpfile semantic/gulpfile.js",
    "semantic:build": "gulp build --gulpfile semantic/gulpfile.js",
    "semantic:watch": "gulp watch --gulpfile semantic/gulpfile.js",
    "semantic": "npm run semantic:build && npm run semantic:watch",
    "test": "cross-env NODE_ENV=test npm run test:unit && npm run test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "serve": "cross-env BUNDLE_ANALYZER=server vue-cli-service serve",
    "postversion": "git push --follow-tags",
    "build": "NODE_ENV=production vue-cli-service build --mode production --modern",
    "start": "vue-cli-service serve --mode local",
    "check": "npm-check --update",
    "deploy": "slseed-utils --do deploy",
    "setup:stack": "slseed-utils --do setup-stack",
    "setup:env": "slseed-utils --do setup-env",
    "setup": "slseed-utils"
  },
